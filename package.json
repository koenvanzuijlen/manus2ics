    "eslint-config-prettier": "8.3.0",
    "husky": "7.0.1",
    "prettier": "2.3.2",
    "standard-version": "9.3.1",
    "ts-loader": "9.2.3",
    "typescript": "4.3.5",
    "webpack": "5.76.0",
    "webpack-cli": "4.7.2",
